prob_distribution <- function(){
# Create a data frame with 1000 random numbers
df <- data.frame(x = rnorm(1000))
# Plot the distribution of the random numbers
ggplot(df, aes(x)) +
geom_histogram(binwidth = 0.25, col = "white", fill = "steelblue") +
theme_minimal() +
theme(legend.position = "none") +
labs(title = "Distribution of 1000 random numbers")
}
